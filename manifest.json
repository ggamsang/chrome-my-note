{
  "mainfest_version": 3,
  "name": "Youtube Video Markdown memo",
  "version": "1.0",
  "permissions": ["storage", "activeTab"],
  "background": {
    "service_worker": "dist/background.js"
  },
  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "ts": ["dist/content.js"]
    }
  ],
  "action": {
    "default_popup": "dist/popup.html",
    "default_script": "dist/popup.js"
  }
}
